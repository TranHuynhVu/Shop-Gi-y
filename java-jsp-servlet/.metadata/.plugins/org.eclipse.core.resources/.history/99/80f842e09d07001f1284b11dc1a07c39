package com.shopgiay.model;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;

@JsonIgnoreProperties(ignoreUnknown = true)
public class DETAIL_SHOES {
	private int ID;
	private int ID_SHOE;
	private double PRICE;
	private int COUNTT;
	private boolean STATUSS;
	public SHOES shoes;
	public SIZES sizes;
	public COLORS colors;
	
 	public SHOES getShoes() {
		return shoes;
	}
	public void setShoes(SHOES shoes) {
		this.shoes = shoes;
	}
	public SIZES getSizes() {
		return sizes;
	}
	public void setSizes(SIZES sizes) {
		this.sizes = sizes;
	}
	public COLORS getColors() {
		return colors;
	}
	public void setColors(COLORS colors) {
		this.colors = colors;
	}
	public DETAIL_SHOES(int iD_SHOE, double pRICE, int cOUNTT, SHOES shoes, SIZES sizes,
			COLORS colors) {
		super();
		ID_SHOE = iD_SHOE;
		PRICE = pRICE;
		COUNTT = cOUNTT;
		this.shoes = shoes;
		this.sizes = sizes;
		this.colors = colors;
	}
//	public DETAIL_SHOES(double pRICE, int cOUNTT, String nAMEESHOES, String sIZE, String nAMEECOLORS) {
//		PRICE = pRICE;
//		COUNTT = cOUNTT;
//		shoes = new SHOES(nAMEESHOES);
//		sizes = new SIZES(sIZE);
//		colors = new COLORS(nAMEECOLORS);
//	}
	public DETAIL_SHOES(int iD, int iD_SHOE, double pRICE, int cOUNTT, boolean sTATUSS) {
		super();
		ID = iD;
		ID_SHOE = iD_SHOE;
		PRICE = pRICE;
		COUNTT = cOUNTT;
		STATUSS = sTATUSS;
	}
	public DETAIL_SHOES() {
		super();
	}
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	public int getID_SHOE() {
		return ID_SHOE;
	}
	public void setID_SHOE(int iD_SHOE) {
		ID_SHOE = iD_SHOE;
	}
	public double getPRICE() {
		return PRICE;
	}
	public void setPRICE(double pRICE) {
		PRICE = pRICE;
	}
	public int getCOUNTT() {
		return COUNTT;
	}
	public void setCOUNTT(int cOUNTT) {
		COUNTT = cOUNTT;
	}
	public boolean isSTATUSS() {
		return STATUSS;
	}
	public void setSTATUSS(boolean sTATUSS) {
		STATUSS = sTATUSS;
	}
	@Override
	public String toString() {
		return "DETAIL_SHOES [ID=" + ID + ", ID_SHOE=" + ID_SHOE + ", PRICE=" + PRICE + ", COUNTT=" + COUNTT
				+ ", STATUSS=" + STATUSS;
	}
	
}
